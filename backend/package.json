    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "fkill": "^7.0.0",
    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "typescript": "^3.8.2",
    "typescript-styled-plugin": "^0.15.0"
